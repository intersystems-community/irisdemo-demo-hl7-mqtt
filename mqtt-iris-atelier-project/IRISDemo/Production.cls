Class IRISDemo.Production Extends IRISDemo.BaseProduction
{

/// Override this in your Production class to do setup before the Production starts
ClassMethod OnStart(pTimeStarted As %String) As %Status
{
		Quit ##class(IRISDemo.Data.Utils).PopulateModel()
}

XData ProductionDefinition
{
<Production Name="IRISDemo.Production" TestingEnabled="true" LogGeneralTraceEvents="true">
  <Description></Description>
  <ActorPoolSize>2</ActorPoolSize>
  <Item Name="EMR HL7 Feed" Category="" ClassName="EnsLib.HL7.Service.FileService" PoolSize="1" Enabled="true" Foreground="false" Comment="" LogTraceEvents="false" Schedule="">
    <Setting Target="Host" Name="MessageSchemaCategory">2.5</Setting>
    <Setting Target="Host" Name="TargetConfigNames">HL7_SMS_Router,EMR HL7 Feed_Router</Setting>
    <Setting Target="Adapter" Name="FilePath">/EMRHL7Feed/FileIn/</Setting>
    <Setting Target="Adapter" Name="FileSpec">*</Setting>
  </Item>
  <Item Name="EMR HL7 Feed_Router" Category="" ClassName="EnsLib.HL7.MsgRouter.RoutingEngine" PoolSize="1" Enabled="true" Foreground="false" Comment="" LogTraceEvents="false" Schedule="">
    <Setting Target="Host" Name="BusinessRuleName">IRISDemo.EMRHL7FeedRoutingRule</Setting>
  </Item>
  <Item Name="AWS SNS Operation" Category="" ClassName="AWS.BO.SNS.Operation" PoolSize="1" Enabled="true" Foreground="false" Comment="" LogTraceEvents="false" Schedule="">
    <Setting Target="Host" Name="FailureTimeout">-1</Setting>
    <Setting Target="Host" Name="ReplyCodeActions">E=R</Setting>
  </Item>
  <Item Name="EMR HL7 Appointment Store Operation" Category="" ClassName="IRISDemo.BO.Appointment.Operation" PoolSize="1" Enabled="true" Foreground="false" Comment="" LogTraceEvents="false" Schedule="">
  </Item>
  <Item Name="EnsLib.JavaGateway.Initiator" Category="" ClassName="EnsLib.JavaGateway.Initiator" PoolSize="1" Enabled="true" Foreground="false" Comment="" LogTraceEvents="false" Schedule="">
    <Setting Target="Host" Name="ClassPath">/usr/irissys/MQTT/jars/org.eclipse.paho.client.mqttv3-1.2.0.jar</Setting>
  </Item>
  <Item Name="MQTTIn" Category="" ClassName="JBH.Antonum.Iris.Mqtt.MqttBS" PoolSize="0" Enabled="true" Foreground="false" Comment="" LogTraceEvents="false" Schedule="">
    <Setting Target="Host" Name="DocSchemaCategory">mqtt_schema_plaintext</Setting>
    <Setting Target="Host" Name="MQTTBrocker">tcp://localhost:1883</Setting>
    <Setting Target="Host" Name="MQTTClientName">IRISInbox</Setting>
    <Setting Target="Host" Name="MQTTTopicRoot">iris/in</Setting>
    <Setting Target="Host" Name="TargetConfigNames">MQTT Heart Rate Routing Rule</Setting>
    <Setting Target="Host" Name="Credentials">MQTTCredential</Setting>
  </Item>
  <Item Name="MQTTOut" Category="" ClassName="JBH.Antonum.Iris.Mqtt.MqttBO" PoolSize="1" Enabled="true" Foreground="false" Comment="" LogTraceEvents="false" Schedule="">
    <Setting Target="Host" Name="MQTTBrocker">tcp://localhost:1883</Setting>
    <Setting Target="Host" Name="MQTTClientName">IRISOutbox</Setting>
    <Setting Target="Host" Name="MQTTTopicRoot">iris/out</Setting>
    <Setting Target="Host" Name="LogFile">/usr/irissys/MQTT/logs/MQTTOut.log</Setting>
    <Setting Target="Host" Name="Credentials">MQTTCredential</Setting>
  </Item>
  <Item Name="MQTT Heart Rate Routing Rule" Category="" ClassName="EnsLib.MsgRouter.VDocRoutingEngine" PoolSize="1" Enabled="true" Foreground="false" Comment="" LogTraceEvents="false" Schedule="">
    <Setting Target="Host" Name="Validation"></Setting>
    <Setting Target="Host" Name="BusinessRuleName">IRISDemo.RR.MQTTHeartRateRouting</Setting>
  </Item>
  <Item Name="Health Information Exchange Observations" Category="" ClassName="EnsLib.HL7.Operation.FileOperation" PoolSize="1" Enabled="true" Foreground="false" Comment="" LogTraceEvents="false" Schedule="">
    <Setting Target="Adapter" Name="FilePath">/tmp</Setting>
    <Setting Target="Host" Name="Filename">HIE_Observation_%f_%Q%!+(_a)</Setting>
  </Item>
</Production>
}

}
